<% provide(:title, "Edit Rune") %> 
<h1 class="banner">Edit Rune</h1>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@rune) do |f| %>
		<%= render 'shared/error_messages', object: f.object %>

		<%= f.label :name %>
		<%= f.text_field :name %>

		<%= f.label :translation %>
		<%= f.text_field :translation %>
		<%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>